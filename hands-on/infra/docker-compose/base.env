# === Local volume root (adjust to avoid syncing folders) ===
DOCKER_VOLUME_ROOT=~/Dev/volumes

# === Database (used by LakeFS metadata) ===
POSTGRES_USER=lakefs
POSTGRES_PASSWORD=__POSTGRES_PASSWORD__
POSTGRES_DB=lakefs

# === LakeFS ===
LAKEFS_LOGGING_LEVEL=INFO
LAKEFS_DATABASE_TYPE=postgres
LAKEFS_STATS_ENABLED=false
LAKEFS_AUTH_ENCRYPT_SECRET_KEY=__LAKEFS_SECRET_KEY__   # override in .env

# === LakeFS Block Storage ===
# For local development, use 'local'. For production, use 's3'
LAKEFS_BLOCKSTORE_TYPE=local
LAKEFS_BLOCKSTORE_LOCAL_PATH=/data/lakefs

# === LakeFS S3/R2 Configuration (when LAKEFS_BLOCKSTORE_TYPE=s3) ===
# Uncomment and set these when using S3/R2 backend
LAKEFS_BLOCKSTORE_S3_BUCKET=mlops-lakefs
LAKEFS_BLOCKSTORE_S3_REGION=us-east-1
LAKEFS_BLOCKSTORE_S3_ENDPOINT=${LAKEFS_BLOCKSTORE_S3_ENDPOINT:-}  # override in .env
LAKEFS_BLOCKSTORE_S3_CREDENTIALS_ACCESS_KEY_ID=${R2_ACCESS_KEY:-}  # override in .env
LAKEFS_BLOCKSTORE_S3_CREDENTIALS_SECRET_ACCESS_KEY=${R2_SECRET_KEY:-}  # override in .env
LAKEFS_BLOCKSTORE_S3_FORCE_PATH_STYLE=true  # Required for R2

# === MLflow ===
MLFLOW_BACKEND_STORE_URI=sqlite:///mlflow.db
MLFLOW_DEFAULT_ARTIFACT_ROOT=/mlflow/artifacts

# === Redis (Feast online store) ===
REDIS_PORT=6379
